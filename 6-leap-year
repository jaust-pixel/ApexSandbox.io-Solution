public Boolean isLeapYear(Integer year) {
    //code here
    Boolean divisibleBy4 = Math.mod(year,4) == 0;
    Boolean divisibleBy100 = Math.mod(year,100) == 0;
    Boolean divisibleBy400 = Math.mod(year, 400) == 0;

    if(!divisibleBy4) {
        return false;
    } else if (!divisibleBy100) {
        if(!divisibleBy400) {
            return false;
        }
        return true;
    } 
    return true;
}

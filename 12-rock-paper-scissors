public Integer rockPaperScissors(String player1, String player2) {
    if (player1.equals(player2)) {
        return 0;
    }
    switch on player1 {
        when 'rock' {
            switch on player2 {
                when 'scissors' {
                    return 1;
                }
                when 'paper' {
                    return 2;
                }
            }
        }
        when 'paper' {
            switch on player2 {
                when 'rock' {
                    return 1;
                }
                when 'scissors' {
                    return 2;
                }
            }
        }
        when 'scissors' {
            switch on player2 {
                when 'paper' {
                    return 1;
                }
                when 'rock' {
                    return 2;
                }
            }
        }
        when else {
            return 0;
        }
    }
}